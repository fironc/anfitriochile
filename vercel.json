{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "anfitrio.cl" }],
      "destination": "https://www.anfitrio.cl/$1",
      "permanent": true
    }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" }
  ]
}
