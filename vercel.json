{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "anfitrio.cl" }],
      "destination": "https://www.anfitrio.cl/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" }
  ]
}
